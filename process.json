{
  "apps": [
    {
      "name": "wedding-website",
      "interpreter" : "babel-node",
      "script": "./server/server.js",
      "restart_delay": 5000,
      "watch": true,
      "ignore_watch" : ["client","static","rethinkdb_data"],
      "log_date_format" : "YYYY-MM-DD HH:mm Z",
      "env": {
         "PORT": 3000,
         "NODE_ENV": "development"
       },
      "env_production": {
         "PORT": 80,
         "NODE_ENV": "production"
       }
    }
  ]
}
